msgstr "NAMES, Lista todos os usuários do canal atual"

#: ../src/common/outbound.c:3296
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <nick> <mensagem>, Envia uma nota CTCP"

#: ../src/common/outbound.c:3297
