msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <nick|host|ip>, 查詢某位使用者的 IP 位址"

#: ../src/common/outbound.c:3389
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <text>，只在本地顯示文字"

