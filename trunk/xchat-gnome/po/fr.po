#: ../src/common/outbound.c:3178
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr ""
"ADDBUTTON <nom> <action>, ajoute un bouton en-dessous de la liste des "
"utilisateurs"

#: ../src/common/outbound.c:3180
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHAN <cmd>, envoie une commande à tous les canaux dans lesquels vous êtes "
"présent"

#: ../src/common/outbound.c:3182
msgid "ALLCHANL <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHANL <cmd>, envoie une commande à tous les canaux dans lesquels vous "
"êtes présent"

#: ../src/common/outbound.c:3184
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <cmd>, envoie une commande à tous les serveurs sur lesquels vous "
"êtes connecté"

#: ../src/common/outbound.c:3185
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<raison>], vous signale comme absent"

#: ../src/common/outbound.c:3187
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <masque> [<typeban>], bannit tous ceux qui correspondent au masque dans "
"le canal en cours. S'ils sont déjà sur le canal ça ne les met pas dehors "
"(kick) (nécessite chanop)"

#: ../src/common/outbound.c:3189
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Efface le texte la fenêtre courante"

#: ../src/common/outbound.c:3190
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Ferme l'onglet/fenêtre courant"

#: ../src/common/outbound.c:3193
msgid "COUNTRY [-s] <code|wildcard>, finds a country code, eg: au = australia"
msgstr ""
"COUNTRY [-s] <code|caractère de substitution>, recherche un code de pays, "
"par exemple : au = australie"

#: ../src/common/outbound.c:3195
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <pseudo> <message>, envoie le message CTCP au pseudo, VERSION et "
"USERINFO sont des messages d'utilisation courante"

#: ../src/common/outbound.c:3197
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, sort du canal courant et revient immédiatement"

#: ../src/common/outbound.c:3199
msgid ""
"\n"
"DCC GET <nick>                      - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file]  - send a file to someone\n"
"DCC PSEND [-maxcps=#] <nick> [file] - send a file using passive mode\n"
"DCC LIST                            - show DCC list\n"
"DCC CHAT <nick>                     - offer DCC CHAT to someone\n"
"DCC PCHAT <nick>                    - offer DCC CHAT using passive mode\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <pseudo>                         - reçoit un fichier proposé\n"
"DCC SEND [-maxcps=#] <pseudo> <fichier>  - envoie un fichier à quelqu'un\n"
"DCC PSEND [-maxcps=#] <pseudo> <fichier> - envoie un fichier en mode passif\n"
"DCC LIST                                 - affiche la liste DCC\n"
"DCC CHAT <pseudo>                        - propose un DCC CHAT à quelqu'un\n"
"DCC PCHAT <pseudo>                       - propose un DCC CHAT en mode "
"passif\n"
"DCC CLOSE <type> <pseudo> <fichier>     exemple :\n"
"         /dcc close send rogerdupont fichier.tar.gz"

#: ../src/common/outbound.c:3211
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <pseudo>, supprime le statut chanhalf-op d'un pseudo sur le canal "
"courant (nécessite chanop)"

#: ../src/common/outbound.c:3213
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <nom>, supprime un bouton de sous la liste des utilisateurs"

#: ../src/common/outbound.c:3215
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <pseudo>, supprime le statut chanop d'un pseudo sur le canal courant "
"(nécessite chanop)"

#: ../src/common/outbound.c:3217
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <pseudo>, supprime le statut voice d'un pseudo sur le canal courant "
"(nécessite chanop)"

#: ../src/common/outbound.c:3218
msgid "DISCON, Disconnects from server"
msgstr "DISCON, se déconnecte du serveur"

#: ../src/common/outbound.c:3219
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <pseudo|hôte|ip>, trouve l'adresse IP d'un utilisateur"

#: ../src/common/outbound.c:3220
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <texte>, affiche un texte en local"

#: ../src/common/outbound.c:3223
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <commande>, exécute une commande. Si l'option -o est utilisée, la "
"sortie est envoyée sur le canal courant, sinon elle est affichée dans la "
"zone de texte courante"

#: ../src/common/outbound.c:3225
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, envoie un SIGCONT au processus"

#: ../src/common/outbound.c:3228
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], tue une exécution en cours dans la session courante. Si -9 "
"est fourni, le processus est tué par SIGKILL"

#: ../src/common/outbound.c:3230
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, envoie un SIGSTOP au processus"

#: ../src/common/outbound.c:3231
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, envoie des données à l'entrée standard du processus"

#: ../src/common/outbound.c:3235
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, vide la queue d'envoi du serveur courant"

#: ../src/common/outbound.c:3237
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <hôte> [<port>], utilise un hôte comme proxy, le port est 23 par défaut"

#: ../src/common/outbound.c:3240
msgid "GHOST <nick> <password>, Kills a ghosted nickname"
msgstr "GHOST <pseudo> <mot de passe>, tue les pseudonymes fantômes"

#: ../src/common/outbound.c:3244
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr "HOP <pseudo>, donne le statut chanhalf-op au pseudo (nécessite chanop)"

#: ../src/common/outbound.c:3245
msgid "ID <password>, identifies yourself to nickserv"
msgstr "ID <mot de passe>, vous identifie au serveur de noms"

#: ../src/common/outbound.c:3247
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <masque> <types..> <options..>\n"
"    masque  - masque d'hôtes à ignorer, par ex. : *!*@*.aol.com\n"
"    types   - types de données à ignorer, un seul ou tous :\n"
"              PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"

#: ../src/common/outbound.c:3254
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <pseudo> [<canal>], invite quelqu'un sur un canal, par défaut le "
"canal courant (nécessite chanop)"

#: ../src/common/outbound.c:3255
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <canal>, rejoint le canal"

#: ../src/common/outbound.c:3257
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <pseudo>, met dehors (kicke) le pseudo du canal courant (nécessite "
"chanop)"

#: ../src/common/outbound.c:3259
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <pseudo>, bannit puis met dehors (kicke) le pseudo du canal courant "
"(nécessite chanop)"

#: ../src/common/outbound.c:3262
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, force une nouvelle vérification de lag (délai)"

#: ../src/common/outbound.c:3264
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <chaîne>, recherche une chaîne dans le tampon."

#: ../src/common/outbound.c:3266
msgid "LOAD [-e] <file>, loads a plugin or script"
msgstr "LOAD [-e] <fichier>, charge un greffon ou un script"

#: ../src/common/outbound.c:3269
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, fait un deop massif sur tous les chanhalf-ops du canal courant "
"(nécessite chanop)"

#: ../src/common/outbound.c:3271
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, fait un deop massif sur tous les chanops du canal courant (nécessite "
"chanop)"

#: ../src/common/outbound.c:3273
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <action>, envoie l'action dans le canal courant (les actions sont écrites "
"à la 3ème personne, comme /me est content)"

#: ../src/common/outbound.c:3276
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, fait un kick massif de tout le monde sauf vous dans le canal courant "
"(nécessite chanop)"

#: ../src/common/outbound.c:3279
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"MOP, fait un op massif sur tous les utilisateurs du canal courant (nécessite "
"chanop)"

#: ../src/common/outbound.c:3280
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <pseudo> <message>, envoie un message privé"

#: ../src/common/outbound.c:3283
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, Liste les pseudos sur le canal courant"

#: ../src/common/outbound.c:3285
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <pseudo> <message>, Envoie une notice CTCP"

#: ../src/common/outbound.c:3286
msgid "NEWSERVER [-noconnect] <hostname> [<port>]"
msgstr "NEWSERVER <nomhôte> [<port>]"

#: ../src/common/outbound.c:3287
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <pseudonyme>, définit votre pseudo"

#: ../src/common/outbound.c:3290
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <pseudo/canal> <message>, envoie une notification. Les notifications "
"sont un type de message auquel on devrait réagir automatiquement"

#: ../src/common/outbound.c:3292
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<pseudo>], liste votre liste de notifications et y ajoute quelqu'un"

#: ../src/common/outbound.c:3294
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr "OP <pseudo>, donne le statut chanop au pseudo (nécessite chanop)"

#: ../src/common/outbound.c:3296
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr ""
"PART [<canal>] [<raison>], quitte le canal, par défaut le canal courant"

#: ../src/common/outbound.c:3298
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <pseudo | canal>, envoie un CTCP ping au pseudo ou au canal"

#: ../src/common/outbound.c:3300
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr ""
"QUERY <pseudo>, ouvre une nouvelle fenêtre de message privé avec quelqu'un"

#: ../src/common/outbound.c:3302
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<raison>], déconnecte du serveur courant"

#: ../src/common/outbound.c:3304
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <texte>, transmet le texte sous forme brute au serveur"

#: ../src/common/outbound.c:3307
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<hôte>] [<port>] [<motdepasse>], peut être appellé juste "
"avec /RECONNECT pour se reconnecter au serveur courant ou avec /RECONNECT "
"ALL pour se reconnecter à tous les serveurs ouverts"

#: ../src/common/outbound.c:3310
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<hôte>] [<port>] [<motdepasse>], peut être appellé juste avec /"
"RECONNECT pour se reconnecter au serveur courant ou avec /RECONNECT ALL pour "
"se reconnecter à tous les serveurs ouverts"

#: ../src/common/outbound.c:3312
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <texte>, transmet le texte sous forme brute vers xchat, comme s'il "
"avait été reçu depuis le serveur irc"

#: ../src/common/outbound.c:3315
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <texte>, envoie un texte à l'objet de la fenêtre courante"

#: ../src/common/outbound.c:3316
msgid "SEND <nick> [<file>]"
msgstr "SEND <pseudo> [<fichier>]"

#: ../src/common/outbound.c:3319
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <hôte> <port> <canal>, se connecte et rejoint un canal"

#: ../src/common/outbound.c:3322
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <hôte> <port> <canal>, se connecte et rejoint un canal"

#: ../src/common/outbound.c:3326
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <hôte> [<port>] [<motpasse>], se connecte à un serveur, le "
"port par défaut est 6667 pour les connexions normales, et 994 pour les "
"connexions ssl"

#: ../src/common/outbound.c:3329
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <hôte> [<port>] [<motpasse>], se connecte à un serveur, le port par "
"défaut est 6667"

#: ../src/common/outbound.c:3331
msgid "SET [-quiet] <variable> [<value>]"
msgstr "SET [-quiet] <variable> [<valeur>] (-quiet = silencieusement)"

#: ../src/common/outbound.c:3332
msgid "SETCURSOR [-|+]<position>"
msgstr "SETCURSOR [-|+]<position>"

#: ../src/common/outbound.c:3336
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<sujet>], définit le sujet si on en fourni un, sinon affiche le sujet "
"actuel"

#: ../src/common/outbound.c:3338
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <masque> [<masque>...], débannit les masques fournis."

#: ../src/common/outbound.c:3339
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <masque> [QUIET]"

#: ../src/common/outbound.c:3340
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <nom>, décharge un greffon ou un script"

#: ../src/common/outbound.c:3341
msgid "URL <url>, opens a URL in your browser"
msgstr "URL <url>, ouvre une URL dans votre navigateur"

#: ../src/common/outbound.c:3343
msgid ""
"USELECT [-a] [-s] <nick1> <nick2> etc, highlights nick(s) in channel userlist"
msgstr ""
"USELECT [-a] [-s] <pseudo1> <pseudo2> etc, met en surbrillance le(s) "
"pseudonyme(s) dans la liste d'utilisateurs du canal"

#: ../src/common/outbound.c:3346
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr "VOICE <pseudo>, donne le statut voice à quelqu'un (nécessite chanop)"

#: ../src/common/outbound.c:3348
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <message>, écrit le message sur tous les canaux"

#: ../src/common/outbound.c:3350
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <message>, envoie le message à tous les chanops du canal courant"

