#: ../src/common/outbound.c:1650
msgid "I need /bin/sh to run!\n"
msgstr "Do działania potrzebuję /bin/sh!\n"

#: ../src/common/outbound.c:1983
msgid "Commands Available:"
msgstr "Dostępne polecenia:"

#: ../src/common/outbound.c:1997
msgid "User defined commands:"
msgstr "Polecenia użytkownika:"

#: ../src/common/outbound.c:2013
msgid "Plugin defined commands:"
msgstr "Komendy zdefiniowane przez wtyczki:"

#: ../src/common/outbound.c:2024
msgid "Type /HELP <command> for more information, or /HELP -l"
msgstr "Wpisz /HELP <polecenie> lub /HELP -l aby uzyskać więcej informacji"

#: ../src/common/outbound.c:2099
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Zignorowano nieznany argument %s."

#: ../src/common/outbound.c:2960
msgid "No such plugin found.\n"
msgstr "Nie znaleziono takiej wtyczki\n"

#: ../src/common/outbound.c:2965
msgid "That plugin is refusing to unload.\n"
msgstr "Nie można deaktywować wtyczki.\n"

#: ../src/common/outbound.c:3200
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <nazwa> <akcja>, dodaje przycisk pod listą użytkownika"

#: ../src/common/outbound.c:3202
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHAN <polecenie>, wysyła polecenie do wszystkich kanałów na których się "
"znajdujesz"

#: ../src/common/outbound.c:3204
msgid "ALLCHANL <cmd>, sends a command to all channels you're in"
msgstr ""
"ALCHANL <polecenie>, wysyła polecenie do wszystkich kanałów, na których się "
"znajdujesz"

#: ../src/common/outbound.c:3206
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <polecenie>, wysyła polecenie do wszystkich serwerów, z którymi "
"jesteś połączony"

#: ../src/common/outbound.c:3207
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<powód>], ustawia status \"Zaraz wracam\""

#: ../src/common/outbound.c:3209
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <maska> [<typ>], blokuje każdego użytkownika na bieżącym kanale, którego "
"nazwa pasuje do wzorca. Jeżeli użytkownik znajduje się obecnie na kanale, "
"nie powoduje to jego wyrzucenia (polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3211
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Czyści aktualne okno tekstowe"

#: ../src/common/outbound.c:3212
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Zamyka aktualne okno/kartę"

#: ../src/common/outbound.c:3215
msgid "COUNTRY [-s] <code|wildcard>, finds a country code, eg: au = australia"
msgstr "COUNTRY [-s] <kod|wildcard>, znajduje kod kraju, np: au = australia"

#: ../src/common/outbound.c:3217
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <nazwa_uzytkownika> <wiadomosc>, wysyła wiadomość CTCP do użytkownika; "
"często wykorzystywanymi wiadomościami są VERSION i USERINFO"

#: ../src/common/outbound.c:3219
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, opuszcza bieżący kanał i natychmiast łączy się z nim ponownie"

#: ../src/common/outbound.c:3221
msgid ""
"\n"
"DCC GET <nick>                      - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file]  - send a file to someone\n"
"DCC PSEND [-maxcps=#] <nick> [file] - send a file using passive mode\n"
"DCC LIST                            - show DCC list\n"
"DCC CHAT <nick>                     - offer DCC CHAT to someone\n"
"DCC PCHAT <nick>                    - offer DCC CHAT using passive mode\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <nazwa_uzytkownika> - zaakceptuj oferowany plik\n"
"DCC SEND [-maxcps=#] <nazwa_uzytkownika> [plik] - wyślij komuś plik\n"
"DCC PSEND [-maxcps=#] <nazwa_uzytkownika> [plik] - wyślij komuś plik w "
"trybie pasywnym\n"
"DCC LIST - pokaż listę DCC\n"
"DCC CHAT <nazwa_uzytkownika> - zaoferuj komuś DCC CHAT\n"
"DCC PCHAT <nazwa_uzytkownika> - zaoferuj komuś DCC CHAT w trybie pasywnym\n"
"DCC CLOSE <typ> <nazwa_uzytkownika> <plik> przykład:\n"
"         /dcc close send jankowalski plik.tar.gz"

#: ../src/common/outbound.c:3233
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <nazwa_uzytkownika>, usuwa uprawnienia pół-operatora danej osobie na "
"bieżącym kanale (polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3235
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <nazwa>, usuwa przycisk spod listy użytkowników"

#: ../src/common/outbound.c:3237
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <nazwa_uzytkownika>, usuwa status operatora kanału osobie na bieżącym "
"kanale (polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3239
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <nazwa_uzytkownika>, usuwa status voice użytkownikowi na bieżącym "
"kanale (polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3240
msgid "DISCON, Disconnects from server"
msgstr "DISCON, przerywa połączenie z serwerem"

#: ../src/common/outbound.c:3241
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <nick|host|ip>, Wyszukuje numer IP użytkownika"

#: ../src/common/outbound.c:3242
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <tekst>, Wyświetla tekst lokalnie"

#: ../src/common/outbound.c:3245
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <polecenie>, uruchamia polecenie. Jeżeli flaga -o jest użyta, dane "
"wyjściowe są przesyłane na aktualny kanał, a w przeciwnym razie są "
"wyświetlane w aktualnej ramce"

#: ../src/common/outbound.c:3247
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, wysyła procesowi sygnał SIGCONT"

#: ../src/common/outbound.c:3250
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], zabija pracujący program w bieżącej sesji. Jeśli użyty "
"zostanie atrybut -9, dany proces jest zabijany przy użyciu sygnału SIGKILL"

#: ../src/common/outbound.c:3252
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, wysyła procesowi sygnał SIGSTOP"

#: ../src/common/outbound.c:3253
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, wysyła dane na standardowe wejście procesu"

#: ../src/common/outbound.c:3257
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, czyści kolejkę wysyłania bieżącego serwera"

#: ../src/common/outbound.c:3259
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <host> [<port>], pośredniczy w połączeniu do serwera, domyślny port to "
"23"

#: ../src/common/outbound.c:3262
msgid "GHOST <nick> <password>, Kills a ghosted nickname"
msgstr ""
"GHOST <nazwa_uzytkownika> <haslo>, usuwa sesję-ducha danego użytkownika"

#: ../src/common/outbound.c:3267
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <nazwa_użytkownika>, nadaje uprawnienia pół-operatora danej osobie "
"(polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3268
msgid "ID <password>, identifies yourself to nickserv"
msgstr "ID <hasło>, identyfikuje cię w usłudze NickServ"

#: ../src/common/outbound.c:3270
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <maska> <typy..> <opcje..>\n"
"    maska - maska hosta, którego chcemy ignorować, np. *!*@*.example.com\n"
"    typy - typy danych, które chcemy ignorować:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    opcje - NOSAVE, QUIET"

#: ../src/common/outbound.c:3277
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <nazwa_użytkownika> [<kanał>], wysyła użytkownikowi zaproszenie na "
"kanał, domyślnie korzystając z nazwy bieżącego kanału (polecenie wymaga "
"status statusu operatora kanału)"

#: ../src/common/outbound.c:3278
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <channel>, dołącza do kanału"

#: ../src/common/outbound.c:3280
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <nazwa_użytkownika>, wyrzuca danego użytkownika z bieżącego kanału "
"(polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3282
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <nazwa_użytkownika>, blokuje dostęp danemu użytkownikowi, a "
"następnie wyrzuca go z bieżącego kanału (polecenie wymaga uprawnień "
"operatora kanału)"

#: ../src/common/outbound.c:3285
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, wymusza sprawdzenie opóźnienia"

#: ../src/common/outbound.c:3287
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr ""
"LASTLOG <łańcuch>, przeszukuje bufor w poszukiwaniu danego ciągu znaków"

#: ../src/common/outbound.c:3289
msgid "LOAD [-e] <file>, loads a plugin or script"
msgstr "LOAD [-e] <plik>, ładuje wtyczkę lub skrypt"

#: ../src/common/outbound.c:3292
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, Usuwa uprawnienia wszystkim obecnym na kanale półoperatorom "
"(polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3294
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, Usuwa uprawnienia wszystkim obecnym na kanale operatorom (polecenie "
"wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3296
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <akcja>, wysyła akcję na bieżący kanał (akcje są wyrażane w trzeciej "
"osobie, np. /me skacze)"

#: ../src/common/outbound.c:3299
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, Wyrzuca z kanału wszystkich użytkowników, oprócz ciebie (polecenie "
"wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3302
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"MOP, Nadaje wszystkim obecnym na kanale użytkownikom uprawnienia operatora "
"(polecenie wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3303
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <nick> <wiadomość>, wysyła prywatną wiadomość"

#: ../src/common/outbound.c:3306
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, wyświetla listę użytkowników kanału"

#: ../src/common/outbound.c:3308
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <nazwa_użytkownika> <wiadomość>, Wysyła informację CTCP"

#: ../src/common/outbound.c:3309
msgid "NEWSERVER [-noconnect] <hostname> [<port>]"
msgstr "NEWSERVER [-noconnect] <nazwa hosta> [<port>]"

#: ../src/common/outbound.c:3310
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <nick>, ustawia nick użytkownika"

#: ../src/common/outbound.c:3313
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <nazwa_użytkownika/kanał> <wiadomość>, wysyła informację. Informacje "
"to typ wiadomości, na który odpowiedź powinna być udzielona automatycznie"

#: ../src/common/outbound.c:3315
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<nazwa_użytkownika>], wyświetla twoją listę powiadomień lub dodaje "
"do niej użytkownika"

#: ../src/common/outbound.c:3317
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <nazwa_użytkownika>, nadaje prawa operatora użytkownikowi (polecenie "
"wymaga uprawnień operatora kanału)"

#: ../src/common/outbound.c:3319
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<kanał>] [<powód>], opuszcza kanał, domyślnie bieżący"

#: ../src/common/outbound.c:3321
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr ""
"PING <nazwa_użytkownika | kanał>, wysyła polecenie CTCP ping to użytkownika "
"lub kanału"

#: ../src/common/outbound.c:3323
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <nazwa_użytkownika>, otwiera nowe okno prywatnej wiadomości"

#: ../src/common/outbound.c:3325
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<powód>], odłącza od serwera"

#: ../src/common/outbound.c:3327
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <tekst>, wysyła do serwera tekst w niesformatowanej postaci"

#: ../src/common/outbound.c:3330
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<host>] [<port>] [<hasło>], Może być wywoływane w ten sam "
"sposób jak /RECONNECT, aby ponownie połączyć się z bieżącym serwerem, lub z /"
"RECONNECT ALL aby ponownie połączyć się z wszystkimi serwerami"

#: ../src/common/outbound.c:3333
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<host>] [<port>] [<hasło>], Może być wywoływane w ten sam sposób "
"jak /RECONNECT aby ponownie połączyć się z bieżącym serwerem, lub z /"
"RECONNECT ALL aby ponownie połączyć się z wszystkimi serwerami"

#: ../src/common/outbound.c:3335
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <tekst>, wysyła niesformatowany tekst do programu xchat tak, jakby "
"został on odebrany od serwera IRC"

